{
  "name": "itty-fetch",
  "version": "0.0.0",
  "description": "An even simpler wrapper around native Fetch to strip boilerplate from your fetching code.",
  "sourceType": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "lint": "npx eslint src",
    "test": "jest --coverage src",
    "verify": "echo 'verifying module...' && yarn test && yarn build",
    "dev": "yarn lint && jest --watch src",
    "coveralls": "cat ./coverage/lcov.info | node node_modules/.bin/coveralls",
    "prerelease": "yarn verify",
    "prebuild": "rimraf dist && mkdir dist",
    "build": "tsup src/itty-fetch.ts --format cjs,esm --minify --dts --clean",
    "prepublishOnly": "yarn build",
    "release": "release --tag --push"
  },
  "keywords": [
    "fetch",
    "api"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kwhitley/itty-fetch.git"
  },
  "author": "Kevin R. Whitley <krwhitley@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kwhitley/itty-fetch/issues"
  },
  "homepage": "https://itty-fetch.dev",
  "devDependencies": {
    "@types/jest": "^28.1.7",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "@typescript-eslint/parser": "^5.36.2",
    "coveralls": "^3.1.1",
    "esbuild": "^0.14.27",
    "eslint": "^8.11.0",
    "eslint-plugin-jest": "^26.1.2",
    "fetch-mock": "^9.11.0",
    "fs-extra": "^10.0.1",
    "isomorphic-fetch": "^3.0.0",
    "jest": "^28.1.3",
    "rimraf": "^3.0.2",
    "ts-jest": "^28.0.8",
    "ts-node": "^10.9.1",
    "tsup": "^6.2.3",
    "typescript": "^4.7.4",
    "uglify-js": "^3.15.3",
    "yarn": "^1.22.18",
    "yarn-release": "^1.10.3"
  }
}
